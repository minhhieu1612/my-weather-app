{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es2020",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    // "noUnusedLocals": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": "./",
  }
}
